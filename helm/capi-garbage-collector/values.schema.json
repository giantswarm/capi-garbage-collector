                "name": {
                    "type": "string"
                },
                "pullPolicy": {
                    "type": "string"
                },
                "registry": {
                    "type": "string"
                },
                "tag": {
                    "type": "string"
                }
            }
        },
        "pod": {
            "type": "object",
            "properties": {
